<div>
  <% protocol.testsuites.each do |suite| %>
  <div class="row">
    <div class="col-md-4 center-text">
      <%= link_to suite.name.html_safe, product_protocol_testsuite_path(product, protocol,suite)%>

      </div>
    <div class="col-md-4">
      <button class="button animate gray set-to-right">
        <%= link_to "Edit", edit_product_protocol_testsuite_path(product,protocol,suite) %>
      </button>
    </div>
    <div class="col-md-4">
      <button class="button animate gray">
        <%= link_to "Delete", product_protocol_testsuite_path(product,protocol,suite), method: :delete, data: {confirm: "Are you sure you want to delete the testsuite?"} %></span>
      </button>
    </div>
  </div>
  <% end %>

</div>
